{
  "image": "docker.io/uwcryo/asp-binder:c305ac4947c4",
  "hostRequirements": {
    "cpus": 4
  },
  "postCreateCommand": "",
  "remoteEnv": {
    "PATH": "${localEnv:PATH}:/srv/StereoPipeline/bin"
  },
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python"
      ],
	"settings": { 
		"python.defaultInterpreterPath": "/srv/conda/envs/notebook/bin/python"
      }
    }
  }
}
